@{
}
@model IdentityViewModel
@if (Model.Roles != null)
{
    <table class="table">
        <thead>
            <tr>
                <th>RoleName</th>
            </tr>
        </thead>
        @foreach (var role in @Model.Roles)
        {
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    </table>
}